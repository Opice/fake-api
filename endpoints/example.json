{
    "/example/*": {
        "request": {
            "methods" : ["GET"]
        },
        "response": {
            "statusCode": 200,
            "Content-Type": "application/json",
            "headers": {
                "X-header-name": "header value"
            },
            "body": {
                "data": {
                    "wildcardValue": "[[[$1]]]"
                }
            }
        }
    }
}